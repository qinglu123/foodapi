<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bdqn.mapper.OrderDetailMapper">
    <insert id="addOrderDetail">
        INSERT INTO `orderdetail`(`detailid`,`orderid`,`productid`,`productcount`)
        VALUES (#{orderDetail.detailId},#{orderDetail.orderId},#{orderDetail.productId},#{orderDetail.productCount})
    </insert>
</mapper>